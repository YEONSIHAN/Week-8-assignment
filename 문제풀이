// 어디서 부터 모르는 건지 모르겠어서 처음부터 읽고 문제 나오는 부분은 그냥 다 풀어 보기로 했습니다
#include <iostream>
using namespace std;

int main(int argc, char const* argv[])
{
	cout << "hello world";

	return 0;
}

#include <iostream>
using namespace std;

int main(int argc, char const* argv[])
{
	int first = 10, second = 20;
	// 다음 줄에 앞에서 선언된 변수 first, second에 각각 10, 20을 저장하는 코드 작성

	cout << first << ", " << second << endl;
	return 0;
}

#include <iostream>
using namespace std;

int main(int argc, char const* argv[])
{
	// char buf[255] = "Hello World!";
	char buf[255] = {"Hello World!"};

	// 다음 줄에 키보드로 부터 Hello World! 를 입력받아 buf 에 저장하는 코드 작성

	cout << buf << endl;

	return 0;
}

#include <iostream>
#include <string>
using namespace std;
//

namespace my {
	int value = 10;
}

namespace your {
	int value = 20;
}

int main(int argc, char const* argv[])
{
	cout << my::value << endl;
	// 다음 줄에 10이 저장된 변수 value 값을 화면에 출력하는 코드 작성 

	cout << your::value << endl;
	// 다음 줄에 20이 저장된 변수 value 값을 화면에 출력하는 코드 작성

	return 0;
}

#include <iostream>
#include <string>
using namespace std;
//

namespace my {
	string value = "hello";
}

namespace your {
	char value[6] = { "world" };
}

int main(int argc, char const* argv[])
{

	cout << my::value << endl;


	cout << your::value << endl;
	return 0;
}

#include <iostream>
using namespace std;

int main()
{
	cout << "Hello\n";
	cout << "World";

	return 0;
}

#include <iostream>
using namespace std;

int main(int argc, char const* argv[])
{
	int a = 65;

	cout << a;

		return 0;
}

#include <iostream>
using namespace std;

int main(int argc, char const* argv[])
{
	//문제가 65를 출력하라는 거였는데 a가 아닌 A는 아스키 코드값으로
	//65라는 기억이 있어서 해봤음
	char a = 'A';
	cout << int(a);
}

#include <iostream>
using namespace std;

int main(int argc, char const* argv[])
{
	//ㅋㅋ 바로 밑에 똑같은 내용이 ㅋㅋㅋㅋ
	int value = 65;  // 'A' 의 ASCII 값은 65 임 
	cout << value << endl;

	// value에 저장된 값을 변수 ch를 선언하고 value의 값을 ch에 복사하는 코드를 작성
	int ch = 65;
	cout << char(ch) << endl;

	return 0;
}

#include <iostream>
using namespace std;

int main(int argc, char const* argv[])
{
	cout << "true" << endl;//문자열 true
	cout << true << endl;// 값 true
	cout << "false" << endl;//문자열 false
	cout << false << endl;//값 false

	return 0;
}
